    "drizzle-orm": "^0.44.7",
    "next": "16.0.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "workflow": "^4.0.1-beta.9",
    "zod": "^4.1.12"
